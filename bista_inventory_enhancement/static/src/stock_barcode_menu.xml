            <div class="col">
                <button class="button_Orchestrator btn btn-block btn-primary mb-4 w-100"
                        t-on-click="() => this.actionService.doAction('bista_inventory_enhancement.action_inventory_barcode_kanban')">
                    Physical Count
                </button>
            </div>
        </xpath>
    </t>
</templates>






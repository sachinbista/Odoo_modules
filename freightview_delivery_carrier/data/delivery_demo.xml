        <record forcecreate= "True" id="freightview_delivery_carrier" model="delivery.carrier">
            <field name="name">Freightview Delivery</field>
            <field name="delivery_type">freightview</field>
            <field name="product_id" ref="product_delivery_freightview"></field>
            <field name="packaging_id" ref="freightview_packaging_custom"/>
            <field name="extra_service_price">10</field>
            <field name="void_shipment">True</field>
            <field name="uom_id" ref='uom.product_uom_lb'/>
            <field name='delivery_uom'>LB</field>

            
            <field name="freightview_carrier_ids" eval="[(6, 0, [ ref('freightview_carrier_code_fxfe'),ref('freightview_carrier_code_echo'), ref('freightview_carrier_code_dylt')])]"/>
        </record>
    </data>

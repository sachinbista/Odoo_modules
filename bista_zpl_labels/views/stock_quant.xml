<odoo>

    <record id="view_stock_quant_tree_editable" model="ir.ui.view">
        <field name="name">view_stock_quant_tree_editable</field>
        <field name="model">stock.quant</field>
        <field name="inherit_id" ref="stock.view_stock_quant_tree_editable"/>
        <field name="arch" type="xml">
            <xpath expr="//tree" position="inside">
                <header>
                    <button type="object" name="print_label" string="Print Label"/>
                </header>
            </xpath>
        </field>
    </record>


    <record id="view_stock_quant_tree_inventory_editable" model="ir.ui.view">
        <field name="name">view_stock_quant_tree_inventory_editable</field>
        <field name="model">stock.quant</field>
        <field name="inherit_id" ref="stock.view_stock_quant_tree_inventory_editable"/>
        <field name="arch" type="xml">
            <xpath expr="//tree//header" position="inside">
                <button type="object" name="print_label" string="Print Label"/>
            </xpath>
        </field>
    </record>


</odoo>
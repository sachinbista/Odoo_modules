<odoo>
    <data>

        <record id="view_picking_form_print_label" model="ir.ui.view">
            <field name="name">view_picking_form</field>
            <field name="model">stock.picking</field>
            <field name="inherit_id" ref="stock.view_picking_form"/>
            <field name="arch" type="xml">
                <xpath expr="//button[@name='action_open_label_layout']" position="replace">
                    <button type="object" name="print_label" string="Print Label"/>
                </xpath>
            </field>
        </record>

    </data>
</odoo>
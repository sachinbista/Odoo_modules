<odoo>
    <data>

        <record id="stock_location_print_label" model="ir.ui.view">
            <field name="name">stock_location_print_label</field>
            <field name="model">stock.location</field>
            <field name="inherit_id" ref="stock.view_location_form"/>
            <field name="arch" type="xml">
                <xpath expr="//sheet" position="before">
                    <header>
                        <button type="object" name="print_label" string="Print Label"/>
                    </header>
                </xpath>
            </field>
        </record>


        <record id="stock_location_print_label_list_view" model="ir.ui.view">
            <field name="name">stock_location_print_label_list_view</field>
            <field name="model">stock.location</field>
            <field name="inherit_id" ref="stock.view_location_tree2"/>
            <field name="arch" type="xml">
                <xpath expr="//tree" position="inside">
                    <header>
                        <button type="object" name="print_label" string="Print Label"/>
                    </header>
                </xpath>
            </field>
        </record>

    </data>
</odoo>
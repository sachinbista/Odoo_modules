<odoo>
    <data>
        <record id="bista_product_product_form_view_inherit" model="ir.ui.view">
            <field name="name">bista.product.product.form.view.inherit</field>
            <field name="model">product.product</field>
            <field name="inherit_id" ref="product.product_normal_form_view"/>
            <field name="arch" type="xml">
                <xpath expr="//field[@name='product_tooltip']" position='after'>
                    <field name="is_package"/>
                </xpath>
            </field>
        </record>
    </data>
</odoo>
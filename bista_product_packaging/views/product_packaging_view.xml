<odoo>
	<data>
		<record id="bista_product_packaging_form_view_inherit" model="ir.ui.view">
            <field name="name">bista.product.packaging.form.view.inherit</field>
            <field name="model">product.packaging</field>
            <field name="inherit_id" ref="product.product_packaging_form_view"/>
            <field name="arch" type="xml">
            	<xpath expr="//field[@name='barcode']" position='after'>
                    <field name="sale_order_id" options="{'no_create': True}"/>
                    <field name="customer_id" options="{'no_create': True}"/>
                    <field name="mo_id" invisible="1"/>
                </xpath>
            </field>
        </record>
	</data>
</odoo>
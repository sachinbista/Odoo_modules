        <menuitem name="Logs" id="go_flow_menu_logs" parent="go_flow_menu_root"/>
        <menuitem id="menu_goflow_error_log" name="GoGlow Error Log" action="goflow_error_logs_action"
                  parent="bista_go_flow.go_flow_menu_logs" sequence="1"/>

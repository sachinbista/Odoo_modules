        <menuitem name="Goflow Order Status Mapping" id="goflow_order_status_mapping_menu"
                  parent="goflow_instance_details_menu"
                  action="goflow_main_menu_action_window" sequence="5"/>
        <menuitem name="Update Product Fields" id="goflow_update_product_fields_menu"
                  parent="goflow_instance_details_menu"
                  action="goflow_main_menu_action_window" sequence="6"/>

        <!--Operations Menus-->
        <menuitem name="Common Logs" id="goflow_common_logs_menu" parent="goflow_operation_menu"
                  action="goflow_common_log_book" sequence="1"/>
        <menuitem name="Perform Operations" id="goflow_perform_operations_menu" parent="goflow_operation_menu"
                  action="action_goflow_import_export_operation" sequence="2"/>
        <menuitem name="Downloaded Data" id="goflow_downloaded_data_menu" parent="goflow_operation_menu"

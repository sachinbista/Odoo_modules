<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <data>
        <!-- Top menu item -->
        <menuitem name="GoFlow" id="go_flow_menu_root" web_icon="bista_go_flow,static/description/goflow_logo.png"
        groups="spring_systems_integration.group_api_user"/>

        <!-- menu categories -->
        <menuitem name="Configuration" id="goflow_configuration_menu" parent="go_flow_menu_root" sequence="1" groups="bista_go_flow.group_go_flow_admin"/>
        <menuitem name="Operations" id="goflow_operation_menu" parent="go_flow_menu_root" sequence="2"/>
        <menuitem name="Catalog" id="goflow_catalog_menu" parent="go_flow_menu_root" sequence="3"/>

        <!-- Configurations Menus -->
        <menuitem name="Instance Details" id="goflow_instance_details_menu" parent="goflow_configuration_menu"/>
        <menuitem name="Instance" id="goflow_instance_menu" parent="goflow_instance_details_menu"
                  action="goflow_main_menu_action_window" sequence="1"/>
        <menuitem name="GoFlow Warehouse" id="goflow_warehouse_menu" parent="goflow_instance_details_menu"
                  action="goflow_warehouse_menu_action_window" sequence="2"/>
        <menuitem name="GoFlow Channel" id="goflow_channel_menu" parent="goflow_instance_details_menu"

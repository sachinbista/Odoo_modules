        <!--<menuitem name="Product Data" id="goflow_product_data_menu" parent="goflow_downloaded_data_menu"-->
        <!--action="goflow_product_data" sequence="1"/>-->
        <menuitem name="Order Data" id="goflow_order_data_menu" parent="goflow_downloaded_data_menu"
                  action="goflow_order_data" sequence="2"/>

        <!--Catalog Menus-->
        <!--<menuitem name="Products" id="goflow_products_menu" parent="goflow_catalog_menu"-->
        <!--action="goflow_product_menu_action_window" sequence="1"/>-->
        <menuitem name="Purchase Orders" id="goflow_purchase_menu" parent="goflow_catalog_menu"
                  action="goflow_purchase_menu_action_window" sequence="2"/>
        <menuitem name="Vendors" id="goflow_vendor_data_menu" parent="goflow_catalog_menu"
                  action="goflow_vendor_data" sequence="3"/>
        <menuitem name="Warehouses" id="goflow_warehouse_menu" parent="goflow_catalog_menu"
                  action="goflow_warehouse_menu_action_window" sequence="4"/>


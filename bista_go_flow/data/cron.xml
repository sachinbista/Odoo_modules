        <record id="ir_cron_goflow_sync_vendors" model="ir.cron">
            <field name="name">GoFlow Sync Vendors</field>
            <field name="model_id" ref="model_goflow_configuration"/>
            <field name="state">code</field>
            <field name="code">model._auto_goflow_sync_vendors_data()</field>
            <field name="interval_number">1</field>
            <field name="interval_type">hours</field>
            <field name="numbercall">-1</field>
            <field name="doall" eval="False"/>
        </record>


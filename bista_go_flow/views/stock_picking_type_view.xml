<odoo>

    <record id="search_view_form_stock_picking_extend_company_grp" model="ir.ui.view">
        <field name="name">stock.picking.type.inherit.repair</field>
        <field name="model">stock.picking.type</field>
        <field name="inherit_id" ref="stock.view_pickingtype_filter"/>
        <field name="arch" type="xml">
            <xpath expr="//filter[@name='groupby_warehouse_id']" position="after">
                <separator>
                    <filter string="Company" name="company_groupby" context="{'group_by': 'company_id'}"/>
                </separator>
            </xpath>
        </field>
    </record>


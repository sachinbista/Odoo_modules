<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <record id="view_account_invoice_report_search_inherit_reference_number" model="ir.ui.view">
        <field name="name">account.invoice.report.search.inherit</field>
        <field name="model">account.invoice.report</field>
        <field name="inherit_id" ref="account.view_account_invoice_report_search"/>
        <field name="arch" type="xml">
            <xpath expr="//filter[@name='user']" position='after'>
                <filter string="Reference Number" name="reference_number"
                        context="{'group_by':'reference_number'}"/>
            </xpath>

        </field>
    </record>


</odoo>



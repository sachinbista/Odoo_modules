<odoo>
	<data>
		<record id="custom_res_config_settings_view_form" model="ir.ui.view">
            <field name="name">res.config.settings.view.form.inherit.mrp.custom</field>
            <field name="model">res.config.settings</field>
            <field name="inherit_id" ref="mrp.res_config_settings_view_form" />
            <field name="arch" type="xml">
               	<xpath expr="//div[@id='mrp_byproduct']" position="inside">
                    <div class="o_setting_left_pane" style="margin-top: 20px;">
                    	<field name="set_finished_product_location" string="Set Finished Product Location"/>
                    </div>
                    <div class="o_setting_right_pane" style="margin-top: 20px;">
                        <label for="set_finished_product_location"/>
                        <div class="text-muted">
                            Open Wizard to set Finished Products Location on Manufacturing Order. 
                        </div>
                    </div>
                </xpath>
            </field>
        </record>
	</data>
</odoo>
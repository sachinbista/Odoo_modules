<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <record id="view_move_line_tree_inherited_base_amount" model="ir.ui.view">
        <field name="name">account.move.line.form.inherited</field>
        <field name="model">account.move.line</field>
        <field name="inherit_id" ref="account.view_move_line_tree"/>
        <field name="arch" type="xml">
            <xpath expr="//field[@name='matching_number']" position="after">
                    <field name="tax_base_amount"/>
            </xpath>
        </field>
    </record>
</odoo>
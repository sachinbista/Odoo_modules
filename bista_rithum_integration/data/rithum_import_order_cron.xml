<?xml version="1.0" encoding='UTF-8'?>

<odoo noupdate="0">
        <!-- Import Orders -->
        <record id="rithum_import_orders_cron" model="ir.cron">
            <field name="name">Rithum Import Order</field>
            <field name="model_id" ref="model_rithum_config"/>
            <field name="state">code</field>
            <field name="code">model.action_import_order()</field>
            <field name="interval_number">1</field>
            <field name="interval_type">days</field>
            <field name="numbercall">-1</field>
            <field name="active" eval="True"/>
        </record>
</odoo>
<?xml version="1.0" encoding='UTF-8'?>
<odoo>
        <!-- Import Products -->
        <record id="rithum_update_product_qty_cron_cron" model="ir.cron">
            <field name="name">Rithum Update Product QTY</field>
            <field name="model_id" ref="model_rithum_config"/>
            <field name="state">code</field>
            <field name="code">model.action_update_product_qty()</field>
            <field name="interval_number">3</field>
            <field name="interval_type">hours</field>
            <field name="numbercall">1</field>
            <field name="active" eval="True"/>
            <field name="doall" eval="False"/>
        </record>
</odoo>
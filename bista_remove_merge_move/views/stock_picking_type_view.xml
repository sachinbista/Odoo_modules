<odoo>
	<data>
		<record id="bista_stock_picking_type_form_view_inherit" model="ir.ui.view">
            <field name="name">bista.stock.picking.type.form.view.inherit</field>
            <field name="model">stock.picking.type</field>
            <field name="inherit_id" ref="stock.view_picking_type_form"/>
            <field name="arch" type="xml">
            	<xpath expr="//field[@name='show_entire_packs']" position='after'>
                    <field name="is_package"
                    	attrs="{'invisible': [('code', '=', 'outgoing')]}"/>
                </xpath>
            </field>
        </record>
	</data>
</odoo>
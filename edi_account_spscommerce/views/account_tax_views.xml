<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <record id="view_account_tax_form_inherit_edi_account" model="ir.ui.view">
        <field name="name">view.account.tax.form.inherit.edi.account</field>
        <field name="model">account.tax</field>
        <field name="inherit_id" ref="account.view_tax_form"/>
        <field name="arch" type="xml">
            <xpath expr="//field[@name='type_tax_use']" position="after">
                <field name="edi_taxcode"/>
            </xpath>
        </field>
    </record>
</odoo>

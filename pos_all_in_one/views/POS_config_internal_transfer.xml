<odoo>
	<data>
		<record id="res_config_settings_view_internal_transfer_form" model="ir.ui.view">
            <field name="name">res.config.settings.view.form.inherit</field>
            <field name="model">res.config.settings</field>
            <field name="inherit_id" ref="point_of_sale.res_config_settings_view_form" />
            <field name="arch" type="xml">
            	<xpath expr="//h2[@name='pos_interface']" position="before">
                    <h2 name="order">Internal Transfer Config</h2>
					<div class="row mt16 o_settings_container">
						<div id="category_reference" class="col-xs-12 col-md-6 o_setting_box">
							<div class="o_setting_left_pane">
								<field name="pos_internal_transfer"/>
							</div>
							<div class="o_setting_right_pane">
								<label for="pos_internal_transfer" string="Internal Stock Transfer"/>
								<div class="text-muted">
									Internal Stock Transfer
								</div>
							</div>
						</div>
					</div>
                </xpath>
            </field>
        </record>
	</data>
</odoo>
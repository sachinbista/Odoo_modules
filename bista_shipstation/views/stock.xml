<?xml version="1.0" encoding="UTF-8"?>
<odoo>
    <data>

        <record id="shipstation_view_picking_withcarrier_out_form" model="ir.ui.view">
            <field name="name">shipstation_view_picking_withcarrier_out_form</field>
            <field name="model">stock.picking</field>
            <field name="inherit_id" ref="delivery.view_picking_withcarrier_out_form"/>
            <field name="arch" type="xml">
                <field name="carrier_id" position="after">
                    <field name="shipstation_service" attrs="{'invisible': [('delivery_type','!=','shipstation')]}"/>
                    <field name="shipstation_service_code" invisible="1" readonly="1" force_save="1"/>
                    <field name="delivery_type" invisible="1"/>
                    <field name="shipstation_order_id" invisible="1" force_save="1"/>
                    <field name="shipstation_order_key" invisible="1" force_save="1"/>
                </field>
            </field>
        </record>

        <record id="stock_picking_type_inherit" model="ir.ui.view">
            <field name="name">stock picking type</field>
            <field name="model">stock.picking.type</field>
            <field name="inherit_id" ref="stock.view_picking_type_form"/>
            <field name="arch" type="xml">
                <field name="barcode" position="after">
                    <field name="is_subcontractor"/>
                </field>
            </field>
        </record>

    </data>
</odoo>

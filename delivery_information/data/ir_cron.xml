<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <record id="ir_cron_astock_pocking_track_id" model="ir.cron">
        <field name="name">Track Fedex Ship</field>
        <field name="model_id" ref="model_stock_picking"/>

<?xml version="1.0" encoding="utf-8"?>
<odoo>
	
	<record id="aged_receivable_report_po_reference" model="account.report.column">
        <field name="name">Po Reference</field>
        <field name="expression_label">po_reference</field>
        <field name="figure_type">none</field>
        <field name="report_id" ref="account_reports.aged_receivable_report" />
        <field name="sequence">0</field>
    </record>
    <record id="aged_receivable_line_po_reference" model="account.report.expression">
        <field name="label">po_reference</field>
        <field name="engine">custom</field>
        <field name="formula">_report_custom_engine_aged_receivable</field>
        <field name="subformula">po_reference</field>
        <field name="report_line_id" ref="account_reports.aged_receivable_line" />
        <field name="auditable" eval="False" />
    </record>	
</odoo>
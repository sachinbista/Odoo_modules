<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <data noupdate="1">
        <record id="seq_shopify_error_log" model="ir.sequence">
            <field name="name">Shopify Error Log</field>
            <field name="code">shopify.error.log</field>
            <field name="prefix">LOG-</field>
            <field name="padding">5</field>
            <field name="company_id" eval="False"/>
        </record>

        <record id="seq_shopify_log" model="ir.sequence">
            <field name="name">Shopify Log</field>
            <field name="code">shopify.log</field>
            <field name="prefix">LOG-</field>
            <field name="padding">5</field>
            <field name="company_id" eval="False"/>
        </record>

        <record id="seq_shopify_financial_workflow" model="ir.sequence">
            <field name="name">Shopify Financial Workflow</field>
            <field name="code">shopify.financial.workflow</field>
            <field name="prefix">SFW-</field>
            <field name="padding">5</field>
            <field name="number_next">1</field>
            <field name="number_increment">1</field>
            <field name="company_id" eval="False"/>
        </record>

        <record id="seq_shopify_payouts" model="ir.sequence">
            <field name="name">Shopify Payouts</field>
            <field name="code">shopify.payout</field>
            <field name="prefix">PAY-</field>
            <field name="padding">5</field>
            <field name="number_next">1</field>
            <field name="number_increment">1</field>
            <field name="company_id" eval="False"/>
        </record>
    </data>
</odoo>

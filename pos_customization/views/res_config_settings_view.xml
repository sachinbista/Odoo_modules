<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <record id="res_config_settings_view_gs1_shipping_label_form" model="ir.ui.view">
        <field name="name">res.config.settings.view.form.inherit.shipallways</field>
        <field name="model">res.config.settings</field>
        <field name="priority" eval="40"/>
        <field name="inherit_id" ref="point_of_sale.res_config_settings_view_form"/>
        <field name="arch" type="xml">
            <xpath expr="//div[@id='order_reference']" position="after">
                <div class="col-12 col-lg-6 o_setting_box">
                    <div class="o_setting_right_pane">
                        <label for="pos_report_notify_user_ids" string="Pos Report Notify User"/>
                        <div class="text-muted">
                            Select Users for Pos Report Notification
                        </div>
                        <div class="content-group mt16">
                            <field name="pos_report_notify_user_ids" widget="many2many_tags"/>
                        </div>
                    </div>
                </div>
            </xpath>
        </field>
    </record>
</odoo>
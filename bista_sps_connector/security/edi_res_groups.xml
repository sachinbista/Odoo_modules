<?xml version="1.0" encoding="utf-8" ?>
<odoo noupdate="0">
     <record id="category_edi_connector" model="ir.module.category">
         <field name="name">EDI(SPS) Connector</field>
         <field name="description">Helps you manage EDI connector information.</field>
         <field name="sequence">100</field>
     </record>
    <record id="group_edi_user" model="res.groups">
        <field name="name">EDI(SPS) User</field>
        <field name="category_id" ref="category_edi_connector"/>
    </record>

    <record id="group_edi_admin" model="res.groups">
        <field name="name">EDI(SPS) Administrator</field>
        <field name="category_id" ref="category_edi_connector"/>
        <field name="implied_ids" eval="[(4, ref('bista_sps_connector.group_edi_user'))]"/>
    </record>
</odoo>

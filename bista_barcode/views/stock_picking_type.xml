<odoo>
    <data>

        <record id="stock_picking_type_form" model="ir.ui.view">
            <field name="name">view_picking_type_form</field>
            <field name="model">stock.picking.type</field>
            <field name="inherit_id" ref="stock.view_picking_type_form"/>
            <field name="arch" type="xml">
                <xpath expr="//field[@name='use_existing_lots']" position="after">
                    <field name="scan_full_lot"/>
                    <field name="scan_full_qty"/>
                    <field name="scan_package"/>
                    <field name="allow_edit"/>
                    <field name="increment_btn"/>
                    <field name="decrement_btn"/>
                </xpath>
            </field>
        </record>


    </data>
</odoo>


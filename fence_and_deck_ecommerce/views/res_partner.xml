<?xml version="1.0" encoding="utf-8"?>
<odoo>
  <data>
    <record id="view_partner_form_fence_and_deck" model="ir.ui.view">
        <field name="name">res.partner.form.fence.and.deck</field>
        <field name="model">res.partner</field>
        <field name="inherit_id" ref="base.view_partner_form"/>
        <field name="arch" type="xml">
            <xpath expr="//notebook//page[@name='sales_purchases']//field[@name='user_id']" position="after">
                <field name="aquirers_to_show"/>
            </xpath>
        </field>
    </record>
  </data>
</odoo>

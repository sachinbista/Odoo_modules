<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <data>
        <!--       stock_picking type-->

        <record id="view_quant_package_form_custo" model="ir.ui.view">
            <field name="name">stock.quant.package.custom.form</field>
            <field name="model">stock.quant.package</field>
            <field name="inherit_id" ref="stock.view_quant_package_form"/>
            <field name="arch" type="xml">
                 <xpath expr="//field[@name='company_id']" position="before">
                    <label for="product_length" string="L X W X H" />
                    <div class="o_row" >
                        <field name="product_length" nolabel="1"/>
                        <field name="product_width" nolabel="1"/>
                        <field name="product_height" nolabel="1"/>
                    </div>
            </xpath>
            </field>
        </record>
    </data>
</odoo>
<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <record id="res_config_settings_view_gauto_print_config_validation" model="ir.ui.view">
        <field name="name">res.config.settings.view.form.inherit.auto_print_config_validation</field>
        <field name="model">res.config.settings</field>
        <field name="priority" eval="40"/>
        <field name="inherit_id" ref="stock.res_config_settings_view_form"/>
        <field name="arch" type="xml">
           <xpath expr="//div[@name='barcode_setting_container']"
                   position="inside">
                <div class="col-12 col-lg-6 o_setting_box" id="auto_print_config_validation_id" >
                    <div class="o_setting_left_pane">
                        <field name="auto_print_config_validation"/>
                    </div>
                    <div class="o_setting_right_pane">
                        <label for="auto_print_config_validation" />
                        <div class="text-muted">
                            Enables Auto Print Package Barcode for Carton flow pick wagon when user do perform put in pack.
                        </div>
                    </div>
                </div>
            </xpath>
        </field>
    </record>
</odoo>



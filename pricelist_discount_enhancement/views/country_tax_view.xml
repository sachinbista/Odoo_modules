<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <record id="country_tax_form_view" model="ir.ui.view">
        <field name="name">country.tax.tree.view</field>
        <field name="model">country.tax</field>
        <field name="arch" type="xml">
            <tree string="State Tax" editable="top">
                <field name="country_id"/>
                <field name="tax_id"/>
            </tree>
        </field>
    </record>

    <record id="action_country_tax" model="ir.actions.act_window">
        <field name="name">Country Tax</field>
        <field name="res_model">country.tax</field>
        <field name="view_mode">tree</field>
    </record>

    <menuitem id="menu_country_tax" name="Country Taxes" action="action_country_tax" parent="account.account_account_menu"/>
</odoo>